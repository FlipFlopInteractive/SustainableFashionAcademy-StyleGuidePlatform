<section id="{{ slugify title }}">
	<div class="row">
		<div class="col-lg-12">
			<h1>{{ title }}</h1>
			{{#markdown}}{{ description }}{{/markdown}}
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
			<div class="panel panel-default">
				<div class="panel-body">
					<a href="./images/{{ main }}.png" rel="lightbox"><img src="./images/{{ main }}.png" class="img-responsive"></a>
				</div>
				<div class="panel-footer text-right">
					<a href="./downloads/{{ main }}.eps"><i class="icon icon_link"></i></a>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		{{#each types }}
		<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
			<div class="panel panel-default">
				<div class="panel-body">
					<a href="./images/{{ this }}.png" rel="lightbox"><img src="./images/{{ this }}.png" class="img-responsive"></a>
				</div>
				<div class="panel-footer text-right">
					<a href="./downloads/{{ this }}.eps"><i class="icon icon_link"></i></a>
				</div>
			</div>
		</div>
		{{/each}}
	</div>
	<div class="row">
		{{#each symbols }}
		<div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
			<div class="panel panel-default">
				<div class="panel-body">
					<a href="./images/{{ this }}.png" rel="lightbox"><img src="./images/{{ this }}.png" class="img-responsive"></a>
				</div>
				<div class="panel-footer text-right">
					<a href="./downloads/{{ this }}.eps"><i class="icon icon_link"></i></a>
				</div>
			</div>
		</div>
		{{/each}}
	</div>
	<div class="row">
		<div class="col-lg-12">
			<hr/>
		</div>
	</div>
</section>